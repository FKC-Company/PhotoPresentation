


<!DOCTYPE html>
<html lang="en">
	{{>header}}

	<body>
		<div class="container mt-5">
			<div class="row">
				<div class="col-md-8 offset-md-2 box-shadow">
					<div class="card shadow-lg p-5 mt-5 bg-white rounded">
						<div class="card-body">
							{{#if msg}}
								{{ msg }}
							{{/if}}

							<form action="/uploadData" method="POST" enctype="multipart/form-data">
								<div class="mb-3">
									<select id="setNameId" name="setNameId">
									</select>
								</div>
								<div class="mb-3">
									<input class="form-control" type="file" id="myImage" name="myImage" accept='image/*' multiple>
								</div>
								<div class="form-group mt-4"> 
									<button type="submit" class="btn btn-success m-1">upload</button>
								</div>
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</body>
</html>

<script>
	const resSet = {{{resSet}}};
	resSet.map(function(item, i)  {
		$("#setNameId").append("<option value="+ item.set_id +">"+ item.set_title +"</option>");
	});

</script>
